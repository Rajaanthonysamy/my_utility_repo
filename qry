inal_docs = []

for doc, sparql_list in zip(retrieved_docs, sparql_queries_per_doc):
    lines = doc["page_content"].strip().split("\n")
    output_lines = []
    example_idx = 0
    
    for line in lines:
        output_lines.append(line)
        if line.startswith("Query Description:"):
            # Append the corresponding SPARQL query
            if example_idx < len(sparql_list):
                output_lines.append(f"SPARQL Query: {sparql_list[example_idx]}")
                example_idx += 1
    
    # Combine back into single string
    final_docs.append("\n".join(output_lines))
